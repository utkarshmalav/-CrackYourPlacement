class Solution {
    public void moveZeroes(int[] nums) {
        int j=nums.length-1,temp;
        int nz = 0;
        for(int i=0;i<nums.length;i++)
        {
            if(nums[i]!=0)
            {
                temp=nums[i];
                nums[i]=nums[nz];
                nums[nz++]=temp;
            }
        }
        
    }
}
